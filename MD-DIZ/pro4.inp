[MD]
steps                                       16900000 !50ns
random_seed                                       1
temperature                                     300
!initial_temperature                             0.1
stepsize                                        2.0 ! 2fts!
bath_coupling                                   100
separate_scaling                                 on
lrf                                              on
!shake_all_hydrogens                             on
!shake_solvent                                   on

[cut-offs]
solute_solute                                    10
solvent_solvent                                  10
solute_solvent                                   10
!q_atom                                           99

[sphere]
shell_radius                                   20.0
shell_force                                    0.01 !Paper

[solvent]
!radial_force                                    60.0
polarisation                                     on
polarisation_force                             20.0

[intervals]
non_bond                                         25
output                                         1000
energy                                          100
trajectory                                    10000

[files]
topology                         ../complexdiz_w.top
restart                          prod4_complexdiz.re
final                          prod4_1complexdiz.re
fep                                     ../diz.fep
trajectory                    prod4_1complexdiz.dcd
energy                         pro4_1complexdiz.en

